{
  "name": "swarm-intelligence",
  "version": "1.1.3",
  "description": "Implement algorithms of swarm intelligence",
  "keywords": [
    "algorithm",
    "PSO",
    "swarm",
    "intelligence"
  ],
  "bugs": {
    "url": "https://github.com/Krivega/swarm-intelligence/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Krivega/swarm-intelligence.git"
  },
  "license": "MIT",
  "author": {
    "name": "Krivega Dmitriy",
    "email": "mr.krivega@gmail.com",
    "url": "https://krivega.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./objectiveFunctions": {
      "types": "./dist/objectiveFunctions/index.d.ts",
      "import": "./dist/objectiveFunctions/index.js",
      "require": "./dist/objectiveFunctions/index.cjs",
      "default": "./dist/objectiveFunctions/index.js"
    },
    "./swarmCreators": {
      "types": "./dist/swarmCreators/index.d.ts",
      "import": "./dist/swarmCreators/index.js",
      "require": "./dist/swarmCreators/index.cjs",
      "default": "./dist/swarmCreators/index.js"
    },
    "./render": {
      "types": "./dist/render/index.d.ts",
      "import": "./dist/render/index.js",
      "require": "./dist/render/index.cjs",
      "default": "./dist/render/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "dist/**/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "vite build",
    "build:demo": "vite build --config vite.demo.config.ts",
    "format": "yarn format:src && yarn format:demo",
    "format:demo": "prettier --write \"demo/**/*.ts\"",
    "format:src": "prettier --write \"src/**/*.ts\"",
    "lint": "yarn lint:ts && yarn lint:src && yarn lint:demo",
    "lint:demo": "eslint demo",
    "lint:src": "eslint src",
    "lint:ts": "tsc --noEmit",
    "prepare": "yarn build && yarn build:demo",
    "prepublishOnly": " yarn lint",
    "release": "standard-version",
    "start": "vite --config vite.demo.config.ts",
    "test": "jest",
    "test:ci": "cross-env CI=true jest",
    "test:coverage": "yarn test:ci --no-cache --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "events-constructor": "^2.1.1",
    "request-animation-runner": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@eslint/compat": "^2.0.2",
    "@krivega/eslint-config": "^1.1.8",
    "@types/jest": "^30.0.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-extended": "^7.0.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^6.0.5"
  },
  "main:src": "src/index.ts"
}
